{
    "name": "guop-vue",
    "version": "0.1.0",
    "private": "true",
    "scripts": {
        "start": "npm run serve",
        "lint": "eslint --fix --ext .js,.vue src",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "dll": "webpack -p --progess --config ./webpack.dll.config.js",
        "changelog": "conventional-changelog -pangular -i CHANGELOG.md -s",
        "analyz": "cross-env ANALYZ=true vue-cli-service buid"
    },
    "gitHooks": {
        "pre-push": "npm run lint",
        "commit-msg": "commitlint --env GIT_PARAMS"
    },
    "config": {
        "commitizen": {
            "path": "cz-customizable"
        }
    },
    "dependencies": {
        "@antv/data-set": "^0.10.2",
        "ant-design-vue": "^1.4.4",
        "axios": "^0.19.0",
        "babel-polyfill": "^6.26.0",
        "clean-webpack-plugin": "^3.0.0",
        "core-js": "^3.6.5",
        "cross-env": "^6.0.3",
        "lodash.clonedeep": "^4.5.0",
        "lodash.pick": "^4.4.0",
        "md5": "^2.2.1",
        "moment": "^2.28.0",
        "nprogress": "^0.2.0",
        "qs": "^6.9.0",
        "vue": "^2.6.10",
        "vue-clipboard2": "^0.3.1",
        "vue-ls": "^3.2.1",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.1",
        "webpack": "^4.41.2"
    },
    "devDependencies": {
        "@babel/plugin-syntax-jsx": "^7.2.0",
